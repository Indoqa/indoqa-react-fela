{
  "name": "indoqa-react-fela-demo",
  "version": "0.11.1-dev",
  "description": "Indoqa React/Redux Typescript Archetype",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:Indoqa/indoqa-react-redux.git"
  },
  "scripts": {
    "analyze": "source-map-explorer ./target/assets/vendors-*",
    "build": "indoqa-webpack ./indoqa-webpack-options.js",
    "clean": "rimraf ./target",
    "compile": "yarn clean && tsc --outdir ./target/js",
    "lint": "tslint --project .",
    "package": "yarn clean && yarn build",
    "postinstall": "yarn run package",
    "start": "indoqa-dev-server ./indoqa-webpack-options.js"
  },
  "dependencies": {
    "fela": "^10.1.3",
    "fela-monolithic": "^10.1.3",
    "fela-plugin-named-keys": "^10.1.3",
    "fela-preset-web": "^10.1.3",
    "formik": "^1.4.3",
    "history": "^4.7.2",
    "i18next": "^13.0.0",
    "i18next-browser-languagedetector": "^2.2.4",
    "immer": "^1.12.1",
    "indoqa-react-app": "^4.2.0",
    "indoqa-react-fela": "next",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.7.0",
    "react-fela": "^10.1.3",
    "react-i18next": "8.4.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.3.3",
    "shortid": "^2.2.14",
    "typesafe-actions": "^2.0.4",
    "yup": "^0.26.10"
  },
  "devDependencies": {
    "@types/history": "^4.7.2",
    "@types/i18next-browser-languagedetector": "^2.0.1",
    "@types/node": "^10.12.19",
    "@types/react": "^16.7.22",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.0.11",
    "@types/react-i18next": "^8.1.0",
    "@types/react-loadable": "^5.5.0",
    "@types/react-redux": "^6.0.11",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-router-redux": "^5.0.17",
    "@types/redux-logger": "^3.0.6",
    "@types/shortid": "^0.0.29",
    "@types/webpack-env": "^1.13.6",
    "@types/yup": "^0.26.7",
    "indoqa-webpack": "^2.2.0",
    "redux-devtools-extension": "^2.13.7",
    "rimraf": "^2.6.2",
    "source-map-explorer": "1.6.0",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "proxy": {
    "/geonames": {
      "target": "http://api.geonames.org",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "pathRewrite": {
        "/geonames": ""
      }
    },
    "/words": {
      "target": "http://api.wordnik.com",
      "pathRewrite": {
        "/words": ""
      }
    }
  }
}
